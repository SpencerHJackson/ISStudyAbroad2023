@model IEnumerable<ISStudyAbroad2023.Models.Student>

@{
    ViewData["Title"] = "Student List";
}

<h1>Student Names</h1>

<table class="table">
@*    <thead>
        <tr>
            <th>
                First Name
            </th>
        </tr>
    </thead>*@
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.FirstName) @Html.DisplayFor(modelItem => item.LastName)
            </td>
        </tr>
}
    </tbody>
</table>
